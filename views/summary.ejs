<html>
  <head>
    <link rel="stylesheet" href="/reset.css">
    <link rel="stylesheet" href="/common.css">
    <link rel="stylesheet" href="/summary.css">
  </head>
  <body>
    <% include header %>

    <main>
      <h1>集計</h1>
      <div class="summaries">
        <% if (form.summaries) {
          form.summaries.forEach(summary => { %>
            <img src="<%= summary.thumbnailUrl %>">
            <p>like率: <%= summary.likeRate %></p>
          <% })
        } %>
      </div>
    </main>
  </body>
</html>
