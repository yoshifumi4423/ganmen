<html>
  <head></head>
  <body>
    <h1>サインアップ</h1>
    
    <form action="/signup" method="POST">
      <input type="text" name="email" placeholder="email">
      <br>
      <input type="password" name="password" placeholder="password">
      <br>
      <button type="submit">サインアップ</button>
    </form>

    <% if(errors) { %>
      <% errors.forEach((error) => { %>
        <p><%= error %></p>
      <% }) %>
    <% } %>
  </body>
</html>
