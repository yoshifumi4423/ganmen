html
  head

  body
    div
      a(href="/signup") サインアップ
      br
      a(href="/login") ログイン
      br
      a(href="/logout") ログアウト
      br
      a(href="/image") 画像投稿

      ul
        each image in images
          li
            p= image.userId
            img(src=image.filename)
            div
              form(action="/skip" method="POST")
                input(type="hidden" name="imageId" value=image.id)
                button(type="submit") SKIP
              form(action="/like" method="POST")
                input(type="hidden" name="imageId" value=image.id)
                button(type="submit") LIKE
